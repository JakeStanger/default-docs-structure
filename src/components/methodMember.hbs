
<div id=".{{rawName}}" class="item">
    {{#resolveOverloads signatures}}
    {{#if ../isStatic}}<span class="modifier">static</span>{{/if}}
    {{#if ../isAbstract}}<span class="modifier">abstract</span>{{/if}}
    {{#if ../isPrivate}}<span class="modifier">private</span>{{/if}}
    {{#if ../isReadonly}}<span class="modifier">readonly</span>{{/if}}
    {{#if ../isGetter}}<span class="modifier">get</span>{{/if}}
    {{#if ../isSetter}}<span class="modifier">set</span>{{/if}}
        <span class="monospace"><a class="item-name method-name" href="#.{{../rawName}}">{{{../name}}}</a>{{#if typeParameters}}<span>&lt;{{{join typeParameters ", "}}}&gt;</span>{{/if}}<span>({{#if isLarge}}<div class="left-item">{{{join parameters ",<br>"}}}</div>{{else}}{{{join parameters ", "}}}{{/if}})</span>{{~#if returnType}}<span><span class="symbol">:</span> {{{returnType}}}</span>{{/if~}}</span>
    {{#if renderSourceFile}}
    <a href={{../loc.sourceFile}}>{{> sourceCode}}</a>
    {{/if}}
    {{#if comment}}
    <div class="docblock">
    {{{comment}}}
    </div>
    {{/if}}
    {{/resolveOverloads}}
</div>